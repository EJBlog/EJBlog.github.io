[  
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title | escape }}",
      "category" : "{{ post.post_category }}",
      "url"      : "{{ page.url }}{{ page.url }}",
      "date"     : "{{ post.date }}",
      "author"   : "{{ post.post_author }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]

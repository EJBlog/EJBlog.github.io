---
[  
  {% for post in site.posts %}
    {
      "title"    : "{{ post.title | escape }}",
      "category" : "{{ post.post_category | escape }}",
      "url"      : "{{post.url | escape }}",
      "page_url" : "{{page.url | escape}}",
      "date"     : "{{ post.date | escape}}",
      "author"   : "{{ post.post_author | escape }}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]
